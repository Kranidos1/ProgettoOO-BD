import java.awt.*;
import java.awt.EventQueue;
import java.awt.Toolkit;

import javax.swing.BorderFactory;
import javax.swing.JFrame;
import javax.swing.JPanel;
import javax.swing.border.EmptyBorder;
import javax.swing.table.DefaultTableModel;
import javax.swing.JTable;

public class StatisticheCorso extends JFrame {

	private JPanel contentPane;
	private GeneralPanel panel;
	
	public StatisticheCorso() {
		
		super("Project GRU");
		setIconImage(Toolkit.getDefaultToolkit().getImage("C:\\Users\\matti\\Desktop\\lastin.png"));
		
		setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		setBounds(100, 100, 517, 555);
		getContentPane().setLayout(null);
		setResizable(false);
		setLocationRelativeTo(null);
		setVisible(true);
		
		panel = new GeneralPanel();
		getContentPane().add(panel);
		
		JTable tableStats = new JTable();
		tableStats.setSize(481, 279);
		Object[] columns = {"NomeCorso","MediaPresenza","MinimoPresenza","MassimoPresenza","RiempimentoMedio"};
		DefaultTableModel model = new DefaultTableModel();
		model.setColumnIdentifiers(columns);
		tableStats.setModel(model);
		tableStats.setBackground(Color.white);
		
		JPanel panelTableStats = new JPanel();
		panelTableStats.setBounds(10, 154, 481, 279);
		panel.add(panelTableStats);
		panelTableStats.setLayout(null);
		panelTableStats.add(tableStats);
		
	}
}
